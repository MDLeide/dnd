<UserControl x:Class="DND.UI.Domain.CardSpace.DefaultView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DND.UI.Domain.CardSpace"
             xmlns:cashewControls="clr-namespace:Cashew.UI.WPF.UserControls;assembly=Cashew.UI.WPF"
             xmlns:cal="http://www.caliburnproject.org"
             xmlns:design="clr-namespace:DND.UI.Design"
             xmlns:cardSpace="clr-namespace:DND.UI.Domain.CardSpace"
             xmlns:wpf="clr-namespace:Cashew.UI.WPF;assembly=Cashew.UI.WPF"
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance Type=design:DesignCardSpaceViewModel, IsDesignTimeCreatable=True}"
             d:DesignHeight="450" d:DesignWidth="800">
    <wpf:RoutedCommandHandlers.Commands>
        <wpf:RoutedCommandHandler RoutedCommand="Open" Command="{Binding Open}"/>
    </wpf:RoutedCommandHandlers.Commands>

    <Canvas x:Name="Canvas">
    </Canvas>
</UserControl>
